package fr.diginamic.numeration;

public enum Saison {
	
	PRINTEMPS("Printemps"),
	ETE("Ete"),
	AUTOMNE("Automne"),
	HIVER("Hiver"),
	
	ORDER1(1),
	ORDER2(2),
	ORDER3(3),
	ORDER4(4);
	private int order;
	private String name;
	
	private Saison(String name, int order) {
		this.name = name;
		this.order = order;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
