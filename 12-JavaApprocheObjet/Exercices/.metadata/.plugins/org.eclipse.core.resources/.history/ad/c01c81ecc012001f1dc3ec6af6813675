package fr.diginamic.numeration;

public enum Saison {
	
	PRINTEMPS("Printemps"),
	ETE("Ete"),
	AUTOMNE("Automne"),
	HIVER("Hiver");
	
	private String name;
	
	private Saison(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
