package ex2;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class CompteCourantTest {

	@Test
	void testDebiterMontant() {
		CompteCourant comptCourant = new CompteCourant(200, 1000);
		
		comptCourant.ajouterMontant(100);
		comptCourant.debiterMontant(50);
		System.out.println(comptCourant.getDecouvert());
		
	}

}
